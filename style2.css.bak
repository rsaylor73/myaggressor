.button1 {
   -moz-box-shadow:inset 0px 1px 0px 0px #dcecfb;
   -webkit-box-shadow:inset 0px 1px 0px 0px #dcecfb;
   box-shadow:inset 0px 1px 0px 0px #dcecfb;
   background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #fff4cb), color-stop(1, #edc64b) );
   background:-moz-linear-gradient( center top, #fff4cb 5%, #edc64b 100% );
   filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#fff4cb', endColorstr='#edc64b');
   background-color:#fff4cb;
   -webkit-border-top-left-radius:0px;
   -moz-border-radius-topleft:0px;
   border-top-left-radius:0px;
   -webkit-border-top-right-radius:0px;
   -moz-border-radius-topright:0px;
   border-top-right-radius:0px;
   -webkit-border-bottom-right-radius:0px;
   -moz-border-radius-bottomright:0px;
   border-bottom-right-radius:0px;
   -webkit-border-bottom-left-radius:0px;
   -moz-border-radius-bottomleft:0px;
   border-bottom-left-radius:0px;
   text-indent:0;
   border:1px solid #edc64b;
   display:inline-block;
   color:#084c74;
   font-family:Verdana;
   font-size:14px;
   font-weight:bold;
   font-style:normal;
   height:25px;
   line-height:25px;
	min-width:100px;
   text-decoration:none;
   text-align:center;
   text-shadow:1px 1px 0px #ffffff;
}
.button1:hover {
   background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #edc64b), color-stop(1, #fff4cb) );
   background:-moz-linear-gradient( center top, #edc64b 5%, #fff4cb 100% );
   filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#edc64b', endColorstr='#fff4cb');
   background-color:#edc64b;
}.button1:active {
   position:relative;
   top:1px;
}








