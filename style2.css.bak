/* This is the outer shell of the box on index.php */
#reservation-box-outline {
	position: absolute;
	top: 40px;
	left: 80px;
	height: 600px;
	width: 240px;
	background: #FFFFFF;
	-moz-border-radius: 20px;
	border-radius: 20px;
	border:1px solid black;
	opacity:0.65;
	filter:alpha(opacity=50); /* For IE8 and earlier */
   overflow: hidden;
}

#reservation-box-inline-wrapper {
	position: absolute;
   top: 60px;
   left: 100px;
   width: 400px;
   overflow: hidden;
}
	#reservation-box-inline {
	   position: float;
   	top: 200px;
	   left: 100px;
   	width: 400px;
	   overflow: hidden;
	}

	#reservation-start-input-box {
	   position: float;
   	top: 200px;
	   left: 250px;
   	width:400px;
	   overflow: hidden;
	}

	.reservation-start-text {
	   font: 12px Arial;
	   color: #000000;
	}

	#reservation-start-input-box-submit {
	   position: float;
		margin: 0 auto;
		margin-left:auto;
	   margin-right:auto;
	   overflow: hidden;
	}


